package chapter02;

public class VariableIntegerType {

	public static void main(String[] args) {
		
		// 정수: byte   short   int(기본타입)  long
		// 		1byte   2byte   4byte          8byte
		
		int num = 100;
		
		byte byteNum1 = 10;
		byte byteNum2 = 127;
		byte byteNum3 = 128; // Type mismatch: cannot convert from int to byte 에러가 뜸
		
	}
}

	public static void main(String[] args) {
		
		aaaMyName("정성균");
		
		String name = "정성균";
		int age = 44;
		double height = 173.8;
		boolean hasBook = false;
		
		
		System.out.println(name);
		
	}
	
	public static void aaaMyName(String nm) {
		

		System.out.println(nm);
		
	}

}
